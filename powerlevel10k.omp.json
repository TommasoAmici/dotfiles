{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#d3d7cf",
          "foreground": "#000000",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#3465a4",
          "foreground": "#e4e4e4",
          "properties": {
            "home_icon": "~",
            "style": "mixed"
          },
          "style": "plain",
          "template": "<parentBackground,background></> \uf07c {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#4e9a06",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ],
          "foreground": "#000000",
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<parentBackground,background></> {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }} ",
          "type": "git"
        },
        {
          "type": "text",
          "style": "plain",
          "template": "<parentBackground,transparent></>"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#689f63",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": true,
            "fetch_package_manager": true
          },
          "style": "plain",
          "template": "<background,parentBackground></> {{ .Major }} \uf898{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ end }} ",
          "type": "node"
        },
        {
          "type": "r",
          "style": "plain",
          "foreground": "blue",
          "background": "lightWhite",
          "template": "<background,parentBackground></> {{ if .Error }}{{ .Error }}{{ else }}{{ .Major  }}.{{ .Minor }}{{ end }} R "
        },
        {
          "foreground": "#193549",
          "background": "#99908a",
          "style": "plain",
          "template": "<background,parentBackground></> {{ if .Error }}{{ .Error }}{{ else }}{{ .Major  }}.{{ .Minor }}{{ end }} \uE7a8 ",
          "type": "rust"
        },
        {
          "background": "#00acd7",
          "foreground": "#111111",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<background,parentBackground></> {{ if .Error }}{{ .Error }}{{ else }}{{ .Major  }}.{{ .Minor }}{{ end }} \ue627 ",
          "type": "go"
        },
        {
          "background": "#FFDE57",
          "foreground": "#111111",
          "properties": {
            "display_mode": "context",
            "fetch_virtual_env": true
          },
          "style": "plain",
          "template": "<background,parentBackground></> {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Major }}.{{ .Minor }}{{ end }} \ue235 ",
          "type": "python"
        },
        {
          "background": "#ffff66",
          "foreground": "#111111",
          "invert_powerline": true,
          "style": "plain",
          "template": "<background,parentBackground></> \uf0ad ",
          "type": "root"
        },
        {
          "background": "#c4a000",
          "foreground": "#000000",
          "properties": {
            "style": "round"
          },
          "style": "plain",
          "template": "<background,parentBackground></> {{ .FormattedMs }} \uf252 ",
          "type": "executiontime"
        },
        {
          "background": "#000000",
          "background_templates": [
            "{{ if gt .Code 0 }}#cc2222{{ end }}"
          ],
          "foreground": "#d3d7cf",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "<background,parentBackground></> {{ if gt .Code 0 }}{{ .Meaning }}{{ else }}\u2714{{ end }} ",
          "type": "exit"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 2
}